{
  "driver": {
    "browser": "chrome",
    "builders": {
      "forBrowser": [
        "chrome"
      ],
      "withCapabilities": [
        {
          "browserName": "chrome",
          "chromeOptions": {
            "args": [
              "incognito"
            ],
            "binary": "/Applications/Google Chrome Canary.app/Contents/MacOS/Google Chrome Canary"
          }
        }
      ]
    }
  },
  "plugins": {
    "view": {
      "module": "nemo-view",
      "arguments": [
        "path:locator"
      ]
    }
  },
  "data": {
    "urlAmazon": "https://www.amazon.com/",
    "urlFacebook": "https://www.facebook.com/",
    "urlGoogle": "https://www.google.com/",
    "urlPayPal": "https://www.paypal.com/"
  }
}
